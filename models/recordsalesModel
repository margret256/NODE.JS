const mongoose = require("mongoose");

const recordstockSchema = new mongoose.Schema({
  FullName: {
    type: String,
    // required: true,
  },
  Email: {
    type: String,
    // required: true,
  },
  PhoneNumber: {
    type: String,
    // required: true,
  },
  Product: {
    type: String,
    // required: true,
  },
  Quantity: {
    type: Number,
    // required: true,
  },
  DeliveryAddress: {
    type: Date,
    // required: true,
  },
});

module.exports = mongoose.model("recordsalesModel", recordsalesSchema);
